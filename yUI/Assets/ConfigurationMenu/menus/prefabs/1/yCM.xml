					<and src="parent()" trait="_enabled"/>
					<and>
						<not src="grandparent()" trait="_Menu"/>
					</and>
				</onlyif>
				<add> &mainmenu; </add>
			</systemcolor>
			<x> <add src="parent()" trait="width"/> <sub> 7.5 </sub> </x>
			<y> <copy src="parent()" trait="height"/> <sub src="me()" trait="height"/> <div> 2 </div> <add> 5 </add> </y>
